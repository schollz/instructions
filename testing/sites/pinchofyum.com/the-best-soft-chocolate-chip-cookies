					$('input[type="search"]').on('awesomplete-open',function(e){
						i++;
						if ( 1 === i ) {
							$(document).trigger('poy-autocomplete-opened');
						}
					});
					var query = '';
					$('input[type="search"]').on('awesomplete-select',function(e){
						query = e.target.value;
					});
					$('input[type="search"]').on('awesomplete-selectcomplete',function(e){
						var val = e.target.value;
						if ( typeof titleLinks[ val ] !== 'undefined' ) {
							var clickE = {
								results_returned: $('.awesomplete ul li').length,
								link_clicked: titleLinks[ val ],
								location: 'Autocomplete List',
								query: query
							};
							$(document).trigger('poy-search-result-clicked', clickE );
							window.location.href = titleLinks[ val ];
						}
					});
				}
			});
