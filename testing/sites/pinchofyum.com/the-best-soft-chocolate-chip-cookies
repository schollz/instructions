	<!-- Script for OneSignal prompting customizations - once every 30 days after 20 seconds on the page -->
	<script data-cfasync='false'>
		(function(){
			window.OneSignal = window.OneSignal || [];
			var notificationPromptDelay = 200000;

			window.OneSignal.push(function() {
				var navigationStart = window.performance.timing.navigationStart;
				var timeNow = Date.now();
				setTimeout(
					promptAndSubscribeUser,
					Math.max(notificationPromptDelay - (timeNow - navigationStart), 0)
				);
			});

			function promptAndSubscribeUser() {
				window.OneSignal.isPushNotificationsEnabled(function(isEnabled) {
					if (!isEnabled) {
						promptForPushNotificationsNowOrAfter30Days();
					}
				});
			}

			function promptForPushNotificationsNowOrAfter30Days() {
				const repromptTimestamp = localStorage.getItem('poy_reprompt_timestamp');
				if (!repromptTimestamp || Date.now() > parseInt(repromptTimestamp)) {
					OneSignal.push(function() {
						OneSignal.registerForPushNotifications();
					});
				}
			}

			OneSignal.push(function() {
				OneSignal.on('notificationPermissionChange', function(permissionChange) {
					const currentPermission = permissionChange.to;
					console.log('New permission state:', currentPermission);
					if (currentPermission === 'default') {
						// 30 days in milliseconds
						const daysInMs = 30 * 24 * 60 * 60 * 1000;
						const timestamp = new Date(Date.now() + daysInMs).getTime();
						localStorage.setItem('poy_reprompt_timestamp', timestamp);
					}
				});
			});
		}())
	</script>

