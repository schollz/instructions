                <section id="ingredients" class="ingredients">
                                                                <h2 class="except-mobile">Ingredients</h2>
                                                <form id="formIngredients" class="form-ingredients" name="addIngredients" action="" method="post">

                                    <ul>
                                                                                                                                                        <li>
                                    <input type="checkbox" id="edit-cb1205343" name="cb1205343" value="1205343" class="form-checkbox" /><label for="edit-cb1205343"><span>1 1/2 cups (225 g) unbleached all-purpose flour</span></label>                                </li>
                                                                                                                                                                                                                            <li>
                                    <input type="checkbox" id="edit-cb1205344" name="cb1205344" value="1205344" class="form-checkbox" /><label for="edit-cb1205344"><span>1/2 tsp baking soda</span></label>                                </li>
                                                                                                                                                                                                                            <li>
                                    <input type="checkbox" id="edit-cb1225861" name="cb1225861" value="1225861" class="form-checkbox" /><label for="edit-cb1225861"><span>Pinch salt  </span></label>                                </li>
                                                                                                                                                                                                                            <li>
                                    <input type="checkbox" id="edit-cb1205345" name="cb1205345" value="1205345" class="form-checkbox" /><label for="edit-cb1205345"><span>3/4 cup (170 g) unsalted butter, melted</span></label>                                </li>
                                                                                                                                                                                                                            <li>
                                    <input type="checkbox" id="edit-cb1205346" name="cb1205346" value="1205346" class="form-checkbox" /><label for="edit-cb1205346"><span>1 cup (210 g) brown sugar</span></label>                                </li>
                                                                                                                                                                                                                            <li>
                                    <input type="checkbox" id="edit-cb1205347" name="cb1205347" value="1205347" class="form-checkbox" /><label for="edit-cb1205347"><span>2 tbsp sugar</span></label>                                </li>
                                                                                                                                                                                                                            <li>
                                    <input type="checkbox" id="edit-cb1225862" name="cb1225862" value="1225862" class="form-checkbox" /><label for="edit-cb1225862"><span>1 egg  </span></label>                                </li>
                                                                                                                                                                                                                            <li>
                                    <input type="checkbox" id="edit-cb1225863" name="cb1225863" value="1225863" class="form-checkbox" /><label for="edit-cb1225863"><span>1 egg yolk</span></label>                                </li>
                                                                                                                                                                                                                            <li>
                                    <input type="checkbox" id="edit-cb1208335" name="cb1208335" value="1208335" class="form-checkbox" /><label for="edit-cb1208335"><span>7 oz (195 g) milk or semisweet chocolate, coarsely chopped</span></label>                                </li>
